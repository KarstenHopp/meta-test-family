#!/bin/bash

TESTING="memcached"
TYPES="rpm docker"
for SERVICE in $TESTING; do
echo "> > > > $SERVICE BASED TESTING"
    for TYPE in $TYPES; do (
        echo "> > > >   $TYPE BASED TESTING"
        cd $SERVICE
        MODULE=$TYPE avocado run ./*.py /usr/share/moduleframework/modulelint.py
    )
    done
done
