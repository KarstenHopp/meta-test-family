#!/bin/bash

TESTING="memcached"
TYPES="rpm docker"
PTH="/usr/share/moduleframework/tools"

for SERVICE in $TESTING; do
echo "> > > > SERVICE: $SERVICE"
    (
        cd $SERVICE
        $PTH/generator.py
        MODULE=$TYPE avocado run -m $PTH/general_multiplex.yaml -- ./*.py $PTH/modulelint.py
    )
done
